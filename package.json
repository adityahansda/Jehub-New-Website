    "db:monitor": "node scripts/monitor-indexes.js",
    "postbuild": "next-sitemap",
    "postinstall": "npm ls typescript || npm install typescript@5.5.4"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@react-icons/all-files": "^4.1.0",
