    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.18",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "5.5.4"
