    "@vercel/speed-insights": "^1.2.0",
    "appwrite": "^18.1.1",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "framer-motion": "^11.11.17",
    "googleapis": "^154.1.0",
    "lucide-react": "^0.525.0",
    "next": "^14.2.18",
    "next-seo": "^6.8.0",
    "next-sitemap": "^4.2.3",
    "node-appwrite": "^17.1.0",
    "nodemailer": "^7.0.5",
